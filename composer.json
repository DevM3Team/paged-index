{
    "name": "m3team/paged-index",
    "description": "Simple function to deal with dynamic frontend's datasources",
    "type": "library",
    "version": "4.0.1",
    "authors": [
        {
            "name": "Ares Bagatin",
            "email": "ares.bagatin@m3team.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.9",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "M3Team\\PagedIndex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M3Team\\PagedIndex\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "M3Team\\PagedIndex\\PagedIndexServiceProvider"
            ]
        }
    }
}
